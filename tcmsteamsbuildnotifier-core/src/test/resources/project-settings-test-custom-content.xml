<?xml version="1.0" encoding="UTF-8"?>
<settings>
  <msteamsNotifications enabled="true">
    <msteamsNotification token="hook.msteams.com" channel="http://localhost/test" enabled="true" format="nvpairs">
      <states>
        <state type="buildStarted" enabled="true" />
      </states>
        <content iconUrl="http://my.url.com/my-icon.png"
                 botName="TeamCity2"
                 showBuildAgent="true"
                 showElapsedBuildTime="true"
                 showCommits="true"
                 showCommitters="true"
                 maxCommitsToDisplay="20"
                showFailureReason="true"/>
    </msteamsNotification>
  </msteamsNotifications>
</settings>

