<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>tcMsTeamsNotifications</name>
    <display-name>Microsoft Teams Notifications</display-name>
    <version>${version}</version>
    <description>Extend TeamCity Notifications to post messages in Microsoft Teams for build events. Adds tabs to the Project and Build pages.</description>
    <vendor>
      <name>Spyder007</name>
      <url>http://www.mattgerega.com/</url>
    </vendor>
  </info>
  <deployment use-separate-classloader="false"/>
</teamcity-plugin>
